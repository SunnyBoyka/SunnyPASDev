@{
    ViewData["Title"] = "Welcome";
}

<script type="text/javascript">

    var indexApp = {
        start_action: function () {
            let start = true;

            Promise.all([
                BasicInfo.start_action("BasicInfo", null),
                nMap.start_action("nMap", null),
                NMapCommand.start_action("NMapCommand",null),
                ScanStatus.start_action("ScanStatus",null),
            ]).then(values => {
                indexApp.afterRender();
            });
        },
        EditCallBack: function (obj) {
            alert(JSON.stringify(obj, null, ' '));
            //FileNomination.doEditFill(obj);
        },
        afterRender: function () {
            theApp.goSomeWhere();

            var short_cuts = [];
            var short_cut = {};

            short_cut = {}; short_cut.id = "the_dfq"; short_cut.text = "What is PAS Suite";
            short_cuts.push(short_cut);

            short_cut = {}; short_cut.id = "nMap"; short_cut.text = "NMap";
            short_cuts.push(short_cut);


            short_cut = {}; short_cut.id = "NMapCommand"; short_cut.text = "NMapCommand";
             short_cuts.push(short_cut);

            short_cut = {}; short_cut.id = "ScanStatus"; short_cut.text = "ScanStatus";
             short_cuts.push(short_cut);

            Banner.Show_ShortCuts(short_cuts);
            setTimeout(() => theApp.AOSInitialize(), 1000);
        },
        MakeAjax:function(e){
            //alert("labamba");
            $.majax({url: config.contextPath + "Home/GetTime" }).done(jData => {
              alert(jData);
            }).fail((xhr, textStatus,err) => alert('request failed\n' + xhr + textStatus))
        },
        MakeAdminAjax:function(e){
            //alert("labamba");
            $.majax({url: config.contextPath + "Home/GetAdminTime" }).done(jData => {
              alert(jData);
            }).fail((xhr, textStatus,err) => alert('request failed\n' + xhr + textStatus))
        }
    }; $(document).ready(() => indexApp.start_action())
</script>

